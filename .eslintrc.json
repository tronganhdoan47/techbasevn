{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "new-cap": ["error", { "properties": false }],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
